QMainWindow {
    background-color: #d1d3d4;
}

QTabBar::tab {
    width: 0;
    height: 0;
    margin: 0;
    padding: 0;
    border: none;
}

leftlayout {
    margin: 0;
    height: 100;
}

QListWidget {
    color: #FFFFFF;
    background-color: #d1d3d4;
}

QListWidget::item:selected {
    background-color: #2ABf9E;
}

QLabel {
    qproperty-alignment: AlignCenter;
}

QPushButton {
    background-color: #2899b6;
    padding: 15px;
    font-size: 15px;
}